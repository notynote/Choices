<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="pane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="choices.MainMenuController">
   <children>
      <Text fx:id="titleText" layoutY="97.0" onMouseEntered="#TitleMouseEnter" onMouseExited="#TitleMouseLeave" onMouseMoved="#RainbowDash" strokeType="OUTSIDE" strokeWidth="0.0" text="Title" textAlignment="CENTER" wrappingWidth="1280.0">
         <font>
            <Font size="60.0" />
         </font>
      </Text>
      <Button fx:id="startButton" layoutX="516.0" layoutY="279.0" mnemonicParsing="false" onAction="#StartGame" prefHeight="88.0" prefWidth="249.0" text="Start Game" />
      <Text fx:id="authorText" layoutX="1.0" layoutY="152.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Author" textAlignment="CENTER" wrappingWidth="1280.0">
         <font>
            <Font size="30.0" />
         </font>
      </Text>
      <Button fx:id="exitButton" layoutX="516.0" layoutY="407.0" mnemonicParsing="false" onAction="#exitProgram" prefHeight="88.0" prefWidth="249.0" text="EXIT" />
      <ImageView fx:id="imageView" fitHeight="235.0" fitWidth="281.0" layoutX="41.0" layoutY="451.0" pickOnBounds="true" preserveRatio="true" />
      <Slider fx:id="volumeSlider" blockIncrement="0.1" layoutX="1092.0" layoutY="664.0" max="1.0" value="0.5" />
      <Text layoutX="1124.0" layoutY="654.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Music Volume" />
      <Text fx:id="volumeText" layoutX="1124.0" layoutY="699.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <CheckBox fx:id="muteCheck" layoutX="1175.0" layoutY="686.0" mnemonicParsing="false" onAction="#MusicToggle" text="Mute" />
   </children>
</AnchorPane>
